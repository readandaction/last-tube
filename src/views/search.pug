extends layouts/main
include mixins/videoBlock 

block content
    .search__header
        h3 Searching for : #{searchingBy}
    .search__videos
        if videos.length === 0
            h1 No videos
        each video in videos
            +videoBlock({
                title:video.title,
                videoFile:video.fileUrl,
                views:video.views,
                id:video.id
            })
            